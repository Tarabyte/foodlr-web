{
  "name": "foodlr-web",
  "version": "0.0.1",
  "description": "Web App for FoodZzilla",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "posttest": "npm run lint",
    "lint": "eslint -c .eslintrc src",
    "server-dev": "better-npm-run server-dev",
    "watch-client": "better-npm-run watch-client",
    "open": "better-npm-run open",
    "tdd": "karma start --autoWatch=true --singleRun=false",
    "dev": "concurrent \"npm run watch-client\" \"npm run server-dev\" \"npm run open\" \"npm run tdd\"",
    "storybook": "start-storybook -p 9002",
    "translations": "better-npm-run translations"
  },
  "betterScripts": {
    "server-dev": {
      "command": "node ./bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node ./bin/dev-server",
      "env": {
        "UV_THREADPOOL_SIZE": 100
      }
    },
    "open": {
      "command": "node ./bin/dev-open",
      "env": {}
    },
    "translations": {
      "command": "node ./bin/dev-translations",
      "env": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tarabyte/foodlr-web.git"
  },
  "keywords": [
    "react",
    "universal",
    "redux"
  ],
  "author": "Tarabanko Yury",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tarabyte/foodlr-web/issues"
  },
  "homepage": "https://github.com/Tarabyte/foodlr-web#readme",
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "http-proxy": "^1.13.3",
    "isomorphic-fetch": "^2.2.1",
    "javascript-serialize": "^1.6.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-icons": "^2.2.1",
    "react-redux": "^4.4.5",
    "react-redux-universal-resolver": "^0.1.1",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "serve-favicon": "^2.3.0",
    "url-loader": "^0.5.7",
    "webpack-isomorphic-tools": "^2.3.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.34.1",
    "autoprefixer": "^6.3.6",
    "babel-eslint": "^6.0.4",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "json-loader": "^0.5.4",
    "isparta-instrumenter-loader": "^1.0.1",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lost": "^6.8.1",
    "mocha": "^2.5.3",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "piping": "^1.0.0-rc.2",
    "postcss-cssnext": "^2.6.0",
    "postcss-font-magician": "^1.4.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-normalize": "^0.3.0",
    "postcss-reporter": "^1.3.3",
    "react-intl": "^2.1.3",
    "react-intl-translations-manager": "^3.0.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "stylelint": "^6.7.1",
    "stylelint-config-standard": "^9.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
